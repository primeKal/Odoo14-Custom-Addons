<odoo>
   
    <!-- Event tree and form View -->
    <record id="view_fans_tree" model="ir.ui.view">
        <field name="name">fans.fans.tree</field>
        <field name="model">fans.fans</field>
        <field name="arch" type="xml">
            <tree string="Fans">
                <field name="name"/>
                <field name="user_id"/>
                <field name="phone"/>
                <field name="state" optional="show"/>

                
            </tree>
        </field>
    </record>




  
